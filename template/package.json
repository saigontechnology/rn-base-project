{
  "name": "RNBaseProject",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "android": "bash ./scripts/run-app.sh android",
    "ios": "bash ./scripts/run-app.sh ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "lint:fix": "eslint . --fix",
    "pretty": "prettier --write \"./**/*.{js,jsx,json}\"",
